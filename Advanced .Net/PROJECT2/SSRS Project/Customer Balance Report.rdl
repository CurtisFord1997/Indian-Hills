  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>TALDataSource</DataSourceName>
        <CommandText>SELECT        TAL_CUSTOMER.CUSTOMER_NAME, TAL_CUSTOMER.CREDIT_LIMIT, TAL_CUSTOMER.BALANCE, ( TAL_CUSTOMER.CREDIT_LIMIT - TAL_CUSTOMER.BALANCE) AS 'Available Balance', CONCAT(RTRIM(TAL_REP.LAST_NAME), ', ', TAL_REP.FIRST_NAME) as 'Rep Name'
FROM            TAL_CUSTOMER INNER JOIN
                         TAL_REP ON TAL_CUSTOMER.REP_NUM = TAL_REP.REP_NUM</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
